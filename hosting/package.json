{
  "name": "hosting",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run firebase",
    "build": "tsc && vite build --mode development",
    "deploy": "firebase deploy --only hosting",
    "preview": "vite preview",
    "firebase": "FIREBASE_FRAMEWORKS_BUILD_TARGET='development' firebase emulators:start --only auth,firestore,hosting --import=./offline-seed --export-on-exit",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "cypress run",
    "test:unit": "mocha",
    "test:debug": "mocha --inspect"
  },
  "devDependencies": {
    "@types/chai": "^5.0.0",
    "@types/mocha": "^10.0.9",
    "@vituum/vite-plugin-handlebars": "^1.1.0",
    "chai": "^5.1.1",
    "cypress": "^13.15.0",
    "cypress-firebase": "^4.2.1",
    "firebase-admin": "^12.6.0",
    "mocha": "^10.7.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "vite": "^5.4.9",
    "vituum": "^1.1.1"
  },
  "dependencies": {
    "@types/jquery": "^3.5.31",
    "firebase": "^10.14.1",
    "firebase-tools": "^13.22.0",
    "firebaseui": "^6.1.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
